{
  "name": "git_hooks-test",
  "version": "1.0.0",
  "description": "An example project to test git hooks",
  "main": "index.ts",
  "bin": {
    "cdk-test": "../git-hook-test/bin/cdk-test.js"
  },
  "scripts": {
    "build": "npx tsc",
    "cdk": "cdk",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts}\"",
    "test": "jest --colors --coverage test",
    "tslint": "tslint \"src/*.ts\"",
    "start": "npm run build && node dist/index.js",
    "start:dev": "nodemon src/index.ts",
    "watch": "tsc -w"
  },
  "pre-push": [
    "prettier",
    "tslint",
    "test"
  ],
  "pre-commit": [
    "prettier",
    "tslint"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/aws-lambda": "^8.10.96",
    "aws-cdk-lib": "^2.22.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "aws-cdk": "2.22.0",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.2",
    "prettier": "2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.0-dev.20220323"
  }
}
