    "axios": "^0.26.1",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "aws-cdk-lib": "2.17.0",
